
#!/bin/sh

set -e

case "$1" in
    purge|remove)
		rm /var/www/lliurex-www/links/llixhoot.json 2>/dev/null
    ;;
    upgrade|failed-upgrade|abort-install|abort-upgrade|disappear)
		echo ""
    ;;

    *)
        echo "postrm called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac

#DEBHELPER#

exit 0
